<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="BEAssistant.TabbedOpciones">
  <!--Pages can be added as references or inline-->
    <ContentPage Title="Control De Procesos">
        <ScrollView>
            <StackLayout>
                <StackLayout Orientation="Horizontal">
                    <Switch Margin="20,25,10,0" HeightRequest="18" WidthRequest="50" x:Name="SwitchInventarioManual" OnColor="Green" ThumbColor="DarkGreen" Toggled="stockManual_Toggled"/>
                    <Label Margin="0,25,20,0" FontSize="18" Text="..." TextColor="Black" FontAttributes="Bold"/>
                </StackLayout>
                <Label Margin="20,5,20,0" FontSize="16" TextColor="Gray"
                Text="..."/>
            </StackLayout>
        </ScrollView>
    </ContentPage>
  <ContentPage Title="Abansdas" />
</TabbedPage>